@model Crud_LuisMoraes.Models.Estabelecimentos

@{
    ViewBag.Title = "Detalhes";
}
<link href="@Url.Content("~/Content/jquery-ui.css")" rel="stylesheet" type="text/css" />
<h2>Delhes do estabelecimento</h2>

<div>

    <hr />
    <dl class="dl-horizontal">

        <div id="accordion">
            <h3>Dados do Estabelecimento</h3>
            <div>
                <p>
        <dt>
            @Html.DisplayName("CNPJ")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cnpj)
        </dd>

        <dt>
            @Html.DisplayName("Razão Social")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.razaoSocial)
        </dd>

        <dt>
            @Html.DisplayName("Nome Fantasia")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nomeFantasia)
        </dd>
        <dt>
            @Html.DisplayName("Categoria")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Categorias.categoria)
        </dd>

        <dt>
            @Html.DisplayName("Status")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.status)
        </dd>

        <dt>
            @Html.DisplayName("Data de Cadastro")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dataCadastro)
        </dd>


        </p>
</div>
<h3>Endereço</h3>
<div>
    <p>


        <dt>
            @Html.DisplayName("Uf")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Uf.estado)
        </dd>





        <dt>
            @Html.DisplayName("Cidade")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cidade)
        </dd>

        <dt>
            @Html.DisplayName("Bairro")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.bairro)
        </dd>

        <dt>
            @Html.DisplayName("Cep")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cep)
        </dd>

        <dt>
            @Html.DisplayName("Logradouro")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.logradouro)
        </dd>

        <dt>
            @Html.DisplayName("Numero")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.numero)
        </dd>

    </p>
</div>
<h3>Dados de Contato</h3>
<div>
    <p>

        <dt>
            @Html.DisplayName("Email")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            @Html.DisplayName("Telefone")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefone)
        </dd>


    </p>

</div>

</div>



    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id }) 
    @Html.ActionLink("Voltar para a lista", "Index")
</p>


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")


    <script type="text/javascript"
            src="~/Scripts/jquery-ui.js">
    </script>


    <script type="text/javascript">
    $(function () {
        $("#accordion").accordion();
    });
    </script>



    <script>
        $(document).ready(function () {
            $("#telefone").inputmask("mask", { "mask": "(99)99999999" });
            $("#cnpj").inputmask("mask", { "mask": "99.999.999/9999-99" }, { reverse: true });
            $("#cep").inputmask("mask", { "mask": "99999-999" });
            $("#nascimento").inputmask("mask", { "mask": "99/99/9999" });
            $("#preco").inputmask("mask", { "mask": "999.999,99" }, { reverse: true });
            $("#valor").inputmask("mask", { "mask": "#.##9,99" }, { reverse: true });
            $("#ip").inputmask("mask", { "mask": "999.999.999.999" });



        });
    </script>
}






